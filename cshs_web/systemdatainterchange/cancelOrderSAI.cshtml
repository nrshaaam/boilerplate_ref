    <script>
        var username = document.getElementsByClassName("name")[0].getElementsByClassName("ng-binding")[0].innerHTML;

        function GetCancelData() {
            var getOrderNo = document.getElementById("OrderNo").value;

            $.ajax({
                url: '../api/CSHSSystemDataInterchange/GetCancelData?OrderNo=' + getOrderNo,
                type: "GET",
                data: {
                },
                dataType: "json",
                success: function (data) {
                    //console.log(data);

                    if (data !== "undefined") {
                        document.getElementById("ttlrcd").innerHTML = data.orderNo;
                        document.getElementById("Label16").innerHTML = data.project;
                        document.getElementById("Label1").innerHTML = data.orderNo;
                        document.getElementById("Label2").innerHTML = data.totalSKU;
                        document.getElementById("Label3").innerHTML = data.totalQty;
                        document.getElementById("Label4").innerHTML = data.orderDate
                        document.getElementById("Label5").innerHTML = data.createdDateTime;
                        document.getElementById("Label17").innerHTML = data.orderStatus;

                        if (data.orderStatus === "Cancel") {
                            document.getElementById("CheckBox1").style.display = "none";
                        }
                        else {
                            document.getElementById("CheckBox1").style.display = "block";
                        }
                    }
                },
                error: function (xhr) {
                    swal("Error", xhr.status + "-" + xhr.statusCode, "error", {
                        buttons: {
                            confirm: true
                        }
                    });
                }
            });
        }

        function CancelConfirm() {
            var getOrderNo = document.getElementById("OrderNo").value;
            $.ajax({
                url: '../api/CSHSSystemDataInterchange/GetCancelConfirm?OrderNo=' + getOrderNo,
                type: "GET",
